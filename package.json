  "author": "akveo <contact@akveo.com>",
  "homepage": "https://github.com/akveo/eva#readme",
  "repository": "git+https://github.com/akveo/eva.git",
  "bugs": {
    "url": "https://github.com/akveo/eva/issues"
  },
  "scripts": {
    "start": "npm run build -- --watch",
    "build": "tsc",
    "clean": "rimraf ./dist",
    "lint": "tslint -p tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "ci:test": "npm test -- --coverage && cat ./dist/jest/coverage/lcov.info | coveralls",
    "test": "jest",
    "gen:kitten": "ts-node ./packages/processor/kitten/scripts/generateExisting",
    "gen:kitten-all": "npm run gen:kitten -- eva",
    "gen:kitten-custom": "ts-node ./packages/processor/kitten/scripts/customize"
  },
  "devDependencies": {
    "@types/jest": "^23.3.11",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^10.12.18",
    "@types/rimraf": "^2.0.2",
    "ajv": "^6.10.0",
    "husky": "^1.1.2",
    "jest": "^24.0.0-alpha.9",
    "lodash.merge": "^4.6.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.3",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  }
}
